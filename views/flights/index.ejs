<%- include("../partials/header") %>


  <table id="list">
    <thead>
      <tr>
        <th>Flight</th>
        <th>Airline</th>
        <th>Airport</th>
        <th>Departure</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% flights.forEach(function(f) { %>
        <tr>
          <td><%= f.flightNo %></td>
          <td><%= f.airline %></td>
          <td><%= f.airport %></td>
          <td><%= f.departs %></td>
          <td><a href="/flights/<%= f._id %>">Details</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
    

<%- include("../partials/footer") %>